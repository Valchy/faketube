	"private": true,
	"dependencies": {
		"@firebase/app": "^0.7.21",
		"@firebase/auth": "^0.19.12",
		"@firebase/firestore": "^3.4.8",
		"axios": "^0.26.1",
		"react": "^18.0.0",
